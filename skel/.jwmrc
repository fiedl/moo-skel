<?xml version="1.0"?>

<JWM>

    <!-- The root menu. -->
    <RootMenu onroot="12">
        <Program icon="terminal.png" label="Terminal">urxvtc</Program>
        <Separator/>
        <Program icon="file-manager.png" label="File Manager">dolphin</Program>
        <Separator/>
        <Menu icon="terminal.png" label="Terminal Shells">
            <Program icon="terminal.png" label="Bash Shell" confirm="false">urxvtc -name "Bash Shell" -title "Bash Shell" -e sh -c "/bin/bash --login"</Program>
            <Program icon="terminal.png" label="Zsh Shell" confirm="false">urxvtc -name "Zsh Shell" -title "Zsh Shell" -e sh -c "/bin/zsh"</Program>
            <Program icon="terminal.png" label="Bourne Shell" confirm="false">urxvtc -name "Bourne Shell" -title "Bourne Shell" -e sh -c "/bin/sh --login"</Program>
        </Menu>
        <Separator/>
        <Menu icon="text-editor.png" label="Programming and Development">
            <Program icon="atom.png" label="Atom Editor">atom</Program>
            <Program icon="meld.png" label="Meld Diff Editor">meld</Program>
            <Program icon="sublime-text.png" label="Sublime Editor">subl3</Program>
            <Program icon="sublime-text.png" label="Sudo Sublime Editor">sudo subl3</Program>
            <Program icon="qtcreator.png" label="Qt Creator">qtcreator</Program>
            <Program icon="gcolor2.png" label="Gcolor2 Picker">gcolor2</Program>
        </Menu>

        <Menu icon="steam.png" label="Games">
                <Program icon="steam.png" label="Steam">SDL_VIDEO_MINIMIZE_ON_FOCUS_LOSS=0 steam --console</Program>
                <Program icon="xonotic.png" label="Xonotic">xonotic</Program>
        </Menu>

        <Menu icon="checkbox.png" label="CLI Utilities">
                <Program icon="gnome-control-center.png" label="CPU Temperature">urxvtc -name "CPUS" -title "CPUS" -e cpus_temp</Program>
                <Program icon="applications-office.png" label="CPU Frequency">urxvtc -name "CPU Freq" -title "CPU Freq" -e cpu_freq</Program>
                <Program icon="menu-editor.png" label="Window Properties">xprop | xmessage -file -</Program>
        </Menu>
        <Menu icon="addressbook.png" label="GUI Utilities">
            <Program icon="pwsafe.png" label="Password Safe Manager">pwsafe</Program>
            <Program icon="preferences-desktop-wallpaper.png" label="Wallpaper Settings">nitrogen</Program>
            <Program icon="gnome-system-monitor.png" label="Task Manager">lxtask</Program>
        </Menu>
            <Menu icon="gnome-nettool.png" label="Networking and Internet">
            <Program icon="liferea.png" label="RSS Syndication">canto-curses</Program>
            <Program icon="evolution.png" label="Mutt Email">mutt</Program>
            <Program icon="filezilla.png" label="Filezilla FTP Client">filezilla -s</Program>
            <Program icon="tightvnc.png" label="TightVNC Viewer">vncviewer</Program>
            <Program icon="tor-browser-en.png" label="Tor Browser">tor-browser-en</Program>
            <Program icon="web-browser.png" label="Firefox Browser">firefox</Program>
            <Program icon="firefox.png" label="Firefox Profile">firefox --profilemanager</Program>
            <Program icon="firefox.png" label="Transmission Torrent Client">transmission-gtk</Program>
        </Menu>
        <Menu icon="preferences-system-firewall.png" label="Network Security">
                <Program icon="etherape.png" label="Etherape Network Analyzer">sudo etherape</Program>
                <Program icon="wireshark.png" label="Wireshark Qt">wireshark-qt</Program>
        </Menu>
        <Menu icon="mplayer.png" label="Video, Audio and Images">
                <Program icon="gimp.png" label="Gimp Image Editor">gimp</Program>
                <Program icon="gpicview.png" label="Image Viewer">gpicview</Program>
                <Program icon="mplayer.png" label="Music On Console Player">mocp</Program>
                <Program icon="kdenlive.png" label="Kdenlive Video Editor">kdenlive</Program>
                <Program icon="vlc.png" label="VLC Video Player">vlc</Program>
                <Program icon="xbmc.png" label="XBMC Media Center">xbmc</Program>
                <Program icon="obs.png" label="Open Broadcasting Studio">obs</Program>
        </Menu>
        <Separator/>
        <Menu icon="window-manager.png" label="Window Manager">
                <Program icon="window-manager.png" label="Reload compositor, devilspie2, wallpapers">wmreload</Program>
                <Program icon="text-editor.png" label="Edit JWM config">atom -n ~/.jwmrc</Program>
                <Restart icon="window-manager.png" label="Reload JWM"/>
                <Exit icon="window-manager.png" label="Exit JWM" confirm="true"/>
        </Menu>
        <Separator/>
        <Menu icon="mooOS.png" label="Package Management">
            <Program icon="update-manager.png" label="Package Updater">urxvtc -name "Package Updater" -title "Package Updater" -e pacupdater</Program>
            <Program icon="octopi.png" label="Octopi Package Manager">octopi</Program>
            <Program icon="update-notifier.png" label="Octopi Update Notifier">octopi-notifier</Program>
        </Menu>
        <Separator/>
        <Menu icon="gnome-session.png" label="System">
                <Program icon="system-suspend.png" label="Lock System">physlock</Program>
                <Program icon="system-restart.png" label="Restart System">systemctl reboot</Program>
                <Program icon="system-shut-down.png" label="Poweroff System">systemctl poweroff</Program>
        </Menu>

    </RootMenu>

    <!-- startup applications -->
    <StartupCommand>nitrogen --restore</StartupCommand>
    <StartupCommand>volumeicon</StartupCommand>
    <StartupCommand>synapse</StartupCommand>
    <StartupCommand>devilspie2_reload</StartupCommand>
    <StartupCommand>compositor_reload</StartupCommand>

    <!-- Options for program groups. -->
    <Group>
        <Class>Pidgin</Class>
        <Option>sticky</Option>
    </Group>
    <Group>
        <Name>xterm</Name>
        <Option>vmax</Option>
        <Option>icon:terminal.png</Option>
    </Group>
    <Group>
        <Name>xedit</Name>
        <Option>icon:editor.png</Option>
    </Group>
    <Group>
        <Name>xclock</Name>
        <Option>desktop:2</Option>
    </Group>
    <Group>
            <Class>URxvt</Class>
            <Class>Steam</Class>
            <Option>nolist</Option>
    </Group>

